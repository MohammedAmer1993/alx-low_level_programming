CC = gcc
SRC = main.c school.c
OBJ = $(SRC:.c=.o)
NAME = school
CFLAGS = -Wall -Werror -Wextra -pedantic
DEPS = m.h


all: $(OBJ) $(DEPS)
	@ $(CC) $(OBJ) -o $(NAME)

clean:
	-@ $(RM) *~ *# *swp $(NAME)

oclean:
	-@ $(RM) *.o

fclean: oclean clean

re: oclean all

m.h:

.PHONY: all clean oclean fclean re
